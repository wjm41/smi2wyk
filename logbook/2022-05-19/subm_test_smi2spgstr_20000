#SBATCH -p ampere
#SBATCH --mail-type=ALL
. /etc/profile.d/modules.sh
module purge
module load rhel8/default-amp
module load miniconda/3
source activate DebiasedMT

onmt_translate -model /rds-d2/user/wjm41/hpc-work/models/smi2wyk/smi2spgstr/model_step_20000.pt -src /home/wjm41/ml_physics/smi2wyk/data/smi2spgstr/src-test.csv -output /home/wjm41/ml_physics/smi2wyk/data/smi2spgstr/pred_step_20000.txt -n_best 10 -beam_size 10 -gpu 0
python /home/wjm41/ml_physics/smi2wyk/smi2wyk/score_predictions.py -targets /home/wjm41/ml_physics/smi2wyk/data/smi2spgstr/tgt-test.csv -beam_size 5 -predictions /home/wjm41/ml_physics/smi2wyk/data/smi2spgstr/pred_step_20000.txt