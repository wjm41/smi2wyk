#!/bin/bash
#SBATCH -J subm_test_smi2spgstr_20000
#SBATCH -A LEE-WJM41-SL2-GPU
#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --gres=gpu:1
#SBATCH --time=1:00:00
#SBATCH --mail-user=wjm41@cam.ac.uk
